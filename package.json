{
  "name": "draehi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "docker:setup": "BUILD_MODE=dev ./scripts/docker-setup.sh",
    "docker:setup:prod": "BUILD_MODE=prod ./scripts/docker-setup.sh",
    "docker:stop": "./scripts/docker-setup.sh --stop",
    "docker:clean": "./scripts/docker-setup.sh --clean",
    "docker:shell:surreal": "docker exec -it draehi-surrealdb bash",
    "docker:shell:keydb": "docker exec -it draehi-keydb bash",
    "docker:shell:minio": "docker exec -it draehi-minio bash",
    "docker:shell:app": "docker exec -it draehi-app bash",
    "db:init": "npx tsx scripts/init-surreal-schema.ts",
    "flush:db": "npx tsx scripts/flush-db.ts",
    "minio": "./scripts/minio.sh",
    "minio:setup": "./scripts/setup-minio.sh"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.13.1",
    "@aws-sdk/client-s3": "^3.932.0",
    "@clerk/nextjs": "^6.35.4",
    "@tailwindcss/typography": "^0.5.19",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "iron-session": "^8.0.3",
    "jose": "^6.1.2",
    "lucide-react": "^0.554.0",
    "marked": "^17.0.0",
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "redis": "^4.7.0",
    "surrealdb": "^1.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
